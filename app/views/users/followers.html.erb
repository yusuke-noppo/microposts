<% provide(:title, @title) %>
  <div class="col-md-8">
    <%= link_to '<<back', user_path %>
    <h3><%= @title %></h3>
      <div class="users follow">
        <% @users.each do |user| %>
          <div class="follow-list">
          <%= gravatar_for(user, size: 50) %>
          <p><%= user.name %></p>
          </div>
        <% end %>
      </div>
  </div>